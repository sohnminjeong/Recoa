<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>

	<typeAliases>
		<typeAlias type="com.recoa.model.vo.Guest" alias="Guest" />
		<typeAlias type="com.recoa.model.vo.ReserveGuest" alias="ReserveGuest" />
		<typeAlias type="com.recoa.model.vo.ReservePaging" alias="ReservePaging"/>
		<typeAlias type="com.recoa.model.vo.User" alias="User" />
		<typeAlias type="com.recoa.model.vo.BoardFree" alias="BoardFree" />
		<typeAlias type="com.recoa.model.vo.BoardNotice" alias="BoardNotice" />
		<typeAlias type="com.recoa.model.vo.Utillbill" alias="Utillbill"/>
	</typeAliases>

	<!-- 쿼리문 -->
	<mappers>
		<mapper resource="mappers/guest-mapper.xml" />
		<mapper resource="mappers/reserveguest-mapper.xml" />
		<mapper resource="mappers/user-mapper.xml"/>
		<mapper resource="mappers/boardFree-mapper.xml"/>
		<mapper resource="mappers/boardNotice-mapper.xml"/>
		<mapper resource="mappers/utillbill-mapper.xml"/>
	</mappers>
</configuration>